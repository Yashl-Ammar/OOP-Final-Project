<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FoodNameUpdateController">
   <children>
      <Pane prefHeight="82.0" prefWidth="600.0" style="-fx-background-color: #D1457D;">
         <children>
            <Label layoutX="43.0" layoutY="28.0" text="Food Name Update" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Label layoutX="52.0" layoutY="141.0" text="Enter New Food Name" />
      <TextField fx:id="FoodNameChange" layoutX="52.0" layoutY="173.0" prefHeight="27.0" prefWidth="433.0" promptText="Food price" style="-fx-background-color: transparent; -fx-border-color: #D1457D; -fx-border-width: 0 0 2 0;" />
      <Button layoutX="433.0" layoutY="272.0" mnemonicParsing="false" onAction="#ChangeButton" prefHeight="27.0" prefWidth="104.0" style="-fx-background-color: #D1457D;" text="Change" textFill="WHITE" />
      <Button layoutX="32.0" layoutY="347.0" mnemonicParsing="false" onAction="#backButton" style="-fx-background-color: #D1457D;" text="Back" textFill="WHITE" />
      <Label fx:id="FoodNameLabelValidity" layoutX="195.0" layoutY="235.0" prefHeight="17.0" prefWidth="156.0" textFill="RED" />
   </children>
</AnchorPane>
