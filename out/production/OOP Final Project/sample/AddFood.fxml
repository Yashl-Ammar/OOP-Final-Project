<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AddFoodRestaurantController">
   <children>
      <Pane prefHeight="92.0" prefWidth="600.0" style="-fx-background-color: #D1457D;">
         <children>
            <Label layoutX="31.0" layoutY="33.0" style="-fx-background-color: transparent; -fx-font-weight: bold;" text="Add Food" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TextField fx:id="AddFoodName" layoutX="151.0" layoutY="146.0" style="-fx-background-color: transparent; -fx-border-color: #D1457D; -fx-border-width: 0px 0px 2px 0px;" />
      <Label layoutX="54.0" layoutY="150.0" prefHeight="17.0" prefWidth="75.0" text="Food Name :" />
      <Label layoutX="56.0" layoutY="214.0" text="Food Price :" />
      <TextField fx:id="AddFoodPrice" layoutX="151.0" layoutY="210.0" style="-fx-border-width: 0px 0px 2px 0px; -fx-border-color: #D1457D; -fx-background-color: transparent;" />
      <Button layoutX="444.0" layoutY="341.0" mnemonicParsing="false" onAction="#addingFood" prefHeight="34.0" prefWidth="123.0" style="-fx-background-color: #D1457D; -fx-font-weight: bold;" text="Add" textFill="WHITE" />
      <Label fx:id="FoodAddLabelValidility" layoutX="127.0" layoutY="288.0" prefHeight="17.0" prefWidth="339.0" textFill="RED" />
      <Button layoutX="28.0" layoutY="358.0" mnemonicParsing="false" onAction="#backtoLoginPage" prefHeight="17.0" prefWidth="105.0" style="-fx-background-color: #D1457D;" text="Back" textFill="WHITE" />
   </children>
</AnchorPane>
