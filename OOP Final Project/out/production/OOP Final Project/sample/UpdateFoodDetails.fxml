<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UpdateFoodDetailsController">
   <children>
      <Pane prefHeight="78.0" prefWidth="600.0" style="-fx-background-color: #D1457D;">
         <children>
            <Label layoutX="24.0" layoutY="26.0" style="-fx-font-weight: bold;" text="Update Food Details" textFill="WHITE">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Label layoutX="30.0" layoutY="144.0" prefHeight="33.0" prefWidth="512.0" text="Enter The Name of Item You want To Update :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="UpdateItemName" layoutX="30.0" layoutY="177.0" prefHeight="33.0" prefWidth="533.0" promptText="Enter Item Name" style="-fx-border-color: #D1457D; -fx-border-width: 0px 0px 2px 0px; -fx-background-color: transparent;" />
      <Button layoutX="432.0" layoutY="231.0" mnemonicParsing="false" onAction="#UpdateButton" prefHeight="25.0" prefWidth="136.0" style="-fx-background-color: transparent; -fx-background-color: #D1457D;" text="Update" textFill="WHITE" />
      <Button layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#backButton" prefHeight="25.0" prefWidth="86.0" style="-fx-background-color: transparent; -fx-background-color: #D1457D;" text="Back" textFill="WHITE" />
      <Label fx:id="UpdateFoodNameLabel" layoutX="198.0" layoutY="256.0" />
   </children>
</AnchorPane>
